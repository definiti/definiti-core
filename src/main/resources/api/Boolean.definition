/**
 * This type represents a boolean as in any other language.
 *
 * It can have only two values: `true` or `false`.
 */
type Boolean {
  /**
   * Returns the inverse of the current `Boolean` (`true` --> `false`, `false` --> `true`)
   */
  not: Boolean

  /**
   * Exclusive or, same as:
   * ```
   * this && !other || !this && other
   * ```
   */
  xor(other: Boolean): Boolean
}
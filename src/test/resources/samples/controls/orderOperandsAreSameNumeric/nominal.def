def anyFunction(string: String): Boolean => {
  string.length > 1
}

type MyType {
  min: Number
  max: Number

  verify {
    "Some test"
    (value) => {
      value.min < value.max + 1.0
    }
  }
}

type MyAlias = Integer {
  verify {
    "Some test"
    (value) => {
      5 <= value && value <= 10
    }
  }
}

verification MyVerification {
  "Some test"
  (value: Number) => {
    value >= 15.5
  }
}